clean:
	rm -f ${ALLOBJS}  rc5des

superclean: clean
	rm -f Makefile buff-in.* buff-out.* *.ini testplat
	find . -name '*~' -exec rm {} \;

distclean: superclean ./configure
	find . -name '.#*' -exec rm {} \;
	rm -f `egrep "^[ ]*\*" ./configure | sed -e 's/\(^.*\*\)\(.*\)\().*$$\)/\2/'`

ident:	rc5des
	ident rc5des | awk '{ printf("%-30s %10s %s %s %s %s%c", $$2,$$3,$$4,$$5,$$6,$$7,10)}'

